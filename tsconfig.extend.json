{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@api": ["./src/api/index"],
      "@api/*": ["./src/api/*"],
      "@core": ["./src/core/index"],
      "@core/*": ["./src/core/*"],
      "@store": ["./src/store/index"],
      "@store/*": ["./src/store/*"],
      "@app": ["./src/modules/app/index"],
      "@app/*": ["./src/modules/app/*"],
      "@calculator": ["./src/modules/calculator/index"],
      "@calculator/*": ["./src/modules/calculator/*"],
      "@currency": ["./src/modules/currency/index"],
      "@currency/*": ["./src/modules/currency/*"]
    }
  }
}